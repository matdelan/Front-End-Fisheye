{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": ["error", "windows"],
        "semi": ["error", "never"],
        "quotes": ["error", "double"],
        //"indent": ["warn","tab"],
        "indent": "off",
        "max-len": ["warn",{ "code": 100 }],
        "no-unused-vars": ["warn", "all"]
    }
}
